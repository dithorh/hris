@model hris.Models.karyawan

<div class="card border-dark">
    <div class="card-header bg-slate header-elements-inline bg-slate-700">
        <h5 class="card-title"><i class="icon-magazine mr-2"></i>Data Pribadi</h5>
        <div class="header-elements">
            <div class="list-icons">
                <a class="list-icons-item" data-action="collapse"></a>
                <a class="list-icons-item" data-action="reload"></a>
                <a class="list-icons-item" data-action="remove"></a>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table">
                <tbody>
                    <tr>
                        <td class="font-weight-semibold">User ID</td>
                        <td>@(Model.user_id)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Nama Karyawan</td>
                        <td>@(Model.nama_karyawan)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Jenis Kelamin</td>
                        <td>@(Model.jenis_kelamin)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Tempat, Tanggal Lahir</td>
                        <td>@(Model.tempat_lahir), @(Model.tgl_lahir)</td>

                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Usia</td>
                        <td>@(Model.usia)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Alamat</td>
                        <td>@(Model.alamat)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Agama</td>
                        <td>@(Model.agama)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Status Perkawinan</td>
                        <td>@(Model.status_perkawinan)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Telp Rumah</td>
                        <td>@(Model.telp_rumah)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">KTP</td>
                        <td>@(Model.ktp)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">Kartu Keluarga</td>
                        <td>@(Model.kartu_keluarga)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">NPWP</td>
                        <td>@(Model.npwp)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">BPJS Kesehatan</td>
                        <td>@(Model.bpjs_kesehatan)</td>
                    </tr>
                    <tr>
                        <td class="font-weight-semibold">BPJS Ketenagakerjaan</td>
                        <td>@(Model.bpjs_ketanagakerjaan)</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
